# move focus
ctrl - h: yabai -m window --focus west
ctrl - j: yabai -m window --focus south
ctrl - k: yabai -m window --focus north
ctrl - l: yabai -m window --focus east

# move focused window
ctrl + alt - h: yabai -m window --swap west
ctrl + alt - j: yabai -m window --swap south
ctrl + alt - k: yabai -m window --swap north
ctrl + alt - l: yabai -m window --swap east

# send window to space
ctrl + alt - 1: yabai -m window --space 1 # editor
ctrl + alt - 2: yabai -m window --space 2 # terminal
ctrl + alt - 3: yabai -m window --space 3 # browser
ctrl + alt - 4: yabai -m window --space 4 
ctrl + alt - 5: yabai -m window --space 5 # chat
ctrl + alt - 6: yabai -m window --space 6 # music

# move focus to display
ctrl - up: yabai -m display --focus next
ctrl - down: yabai -m display --focus prev

# send window to display and move focus
ctrl + alt - up: yabai -m window --display next; yabai -m display --focus next
ctrl + alt - down: yabai -m window --display prev; yabai -m display --focus prev
